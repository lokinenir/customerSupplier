#%RAML 1.0 Trait
description: It defines the client Id Enforcement Policy RAML specification.
headers:
  client_id:
    type: string
    required: true
  client_secret:
    type: string
    required: true
responses:
  401:
    description: Unauthorized or invalid client application credentials
  500:
    description: Bad response from authorization server, or WSDL SOAP Fault error