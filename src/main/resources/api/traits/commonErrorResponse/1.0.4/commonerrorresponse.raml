#%RAML 1.0 DataType

type: object
description: This is generic common error handling response
properties:
  correlationId:
    type: string
    example: 1fsfs34-1fsfs84-1fsfs32-1fsfs54
    required: false
  errorCode:
    type: string
    example: ERR-400
  errorMessage:
    type: string
    example: Bad Request, received message is not as per the contract
  timestamp:
    type: datetime
    example: 2016-02-28T16:41:41.090Z